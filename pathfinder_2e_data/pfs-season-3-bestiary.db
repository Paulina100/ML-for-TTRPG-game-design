ensive"},"actionType":{"value":"action"},"actions":{"value":1},"description":{"value":"<p>The shadow guardian becomes shrouded in shadows and becomes @UUID[Compendium.pf2e.conditionitems.DmAIPqOBomZ7H95W]{Concealed}. As the nature of this effect leaves the guardian's location obvious, it can't use this concealment to Hide or Sneak. This effect lasts for 1 minute or until it is exposed to direct sunlight, whichever comes first.</p>"},"requirements":{"value":""},"rules":[],"slug":null,"source":{"value":""},"traits":{"rarity":"common","value":["illusion","occult","shadow"]},"trigger":{"value":""},"schema":{"version":0.827,"lastMigration":null}},"type":"action"},{"_id":"e3yVen7RiFeaVHWi","flags":{"core":{"sourceId":"Compendium.pf2e.bestiary-ability-glossary-srd.Tkd8sH4pwFIPzqTr"}},"img":"systems/pf2e/icons/actions/OneAction.webp","name":"Grab","sort":700000,"system":{"actionCategory":{"value":"offensive"},"actionType":{"value":"action"},"actions":{"value":1},"description":{"value":"<p>@Localize[PF2E.NPC.Abilities.Glossary.Grab]</p>"},"requirements":{"value":""},"rules":[],"slug":"grab","source":{"value":"Pathfinder Bestiary"},"traits":{"rarity":"common","value":[]},"trigger":{"value":""},"weapon":{"value":""},"schema":{"version":0.827,"lastMigration":null}},"type":"action"},{"_id":"y2u0kialngm00ogh","img":"systems/pf2e/icons/default-icons/lore.svg","name":"Athletics","sort":800000,"system":{"description":{"value":""},"mod":{"value":15},"proficient":{"value":0},"rules":[],"slug":null,"source":{"value":""},"traits":{"rarity":"common","value":[]},"schema":{"version":0.827,"lastMigration":null}},"type":"lore"}],"name":"Deep Shadow Guardian","system":{"abilities":{"cha":{"mod":-5},"con":{"mod":5},"dex":{"mod":1},"int":{"mod":-5},"str":{"mod":5},"wis":{"mod":1}},"attributes":{"ac":{"details":"(20 when broken);  construct armor;","value":24},"allSaves":{"value":""},"hp":{"details":"Hardness 10","max":72,"temp":0,"value":72},"immunities":[{"type":"bleed"},{"type":"death-effects