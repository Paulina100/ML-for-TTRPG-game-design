fects":{"value":[]},"bonus":{"value":35},"damageRolls":{"wgk3oaoywylyu7687zan":{"damage":"4d6+18","damageType":"slashing"}},"description":{"value":""},"rules":[],"slug":null,"source":{"value":""},"traits":{"rarity":"common","value":["agile","magical","reach-10"]},"weaponType":{"value":"melee"},"schema":{"version":0.827,"lastMigration":null}},"type":"melee"},{"_id":"6z71izdol3e13koh","img":"systems/pf2e/icons/default-icons/melee.svg","name":"Goblinfire","sort":1100000,"system":{"attack":{"value":""},"attackEffects":{"custom":"","value":[]},"bonus":{"value":35},"damageRolls":{"bV59ZadzDAYkf29W":{"damage":"4d6+18","damageType":"fire"}},"description":{"value":""},"rules":[],"slug":null,"source":{"value":""},"traits":{"rarity":"common","value":["fire","range-increment-60"]},"weaponType":{"value":"ranged"},"schema":{"version":0.827,"lastMigration":null}},"type":"melee"},{"_id":"W6ypDZ4KL8XzCx7C","flags":{"core":{"sourceId":"Compendium.pf2e.bestiary-ability-glossary-srd.qCCLZhnp2HhP3Ex6"}},"img":"systems/pf2e/icons/actions/Passive.webp","name":"Darkvision","sort":1200000,"system":{"actionCategory":{"value":"interaction"},"actionType":{"value":"passive"},"actions":{"value":null},"description":{"value":"<p>@Localize[PF2E.NPC.Abilities.Glossary.Darkvision]</p>"},"requirements":{"value":""},"rules":[],"slug":"darkvision","source":{"value":"Pathfinder Bestiary"},"traits":{"rarity":"common","value":[]},"trigger":{"value":""},"weapon":{"value":""},"schema":{"version":0.827,"lastMigration":null}},"type":"action"},{"_id":"hvib7p262mdkyhpf","img":"systems/pf2e/icons/actions/Reaction.webp","name":"Exit Stage","sort":1300000,"system":{"actionCategory":{"value":"defensive"},"actionType":{"value":"reaction"},"actions":{"value":null},"deathNote":true,"description":{"value":"<p><strong>Trigger</strong> Taljjae is reduced to 0 Hit Points</p>\n<hr />\n<p><strong>Effect</strong> Taljjae retains 1 Hit Point and teleports to an empty space within 30 feet. One of its masks remains behind and is