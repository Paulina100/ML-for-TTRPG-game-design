"},"actionType":{"value":"action"},"actions":{"value":2},"description":{"value":"<p>Rajna focuses her ki power to allow her to fold her immediate reality and reach a distant opponent. She makes a fist Strike against a creature within 15 feet.</p>"},"requirements":{"value":""},"rules":[],"slug":null,"source":{"value":""},"traits":{"rarity":"common","value":["conjuration","occult"]},"trigger":{"value":""},"weapon":{"value":""},"schema":{"version":0.827,"lastMigration":null}},"type":"action"},{"_id":"ZxPtq2Nl3SOEsWwX","flags":{"core":{"sourceId":"Compendium.pf2e.bestiary-ability-glossary-srd.BCLvAx4Pz4MLa2pu"}},"img":"systems/pf2e/icons/actions/OneAction.webp","name":"Knockdown","sort":2000000,"system":{"actionCategory":{"value":"offensive"},"actionType":{"value":"action"},"actions":{"value":1},"description":{"value":"<p>@Localize[PF2E.NPC.Abilities.Glossary.Knockdown]</p>"},"requirements":{"value":""},"rules":[],"slug":"knockdown","source":{"value":"Pathfinder Bestiary"},"traits":{"rarity":"common","value":[]},"trigger":{"value":""},"weapon":{"value":""},"schema":{"version":0.827,"lastMigration":null}},"type":"action"},{"_id":"UPSRS9I1c31LFaXw","img":"systems/pf2e/icons/default-icons/lore.svg","name":"Acrobatics","sort":2100000,"system":{"description":{"value":""},"mod":{"value":26},"proficient":{"value":0},"rules":[],"slug":null,"source":{"value":""},"traits":{"rarity":"common","value":[]},"schema":{"version":0.827,"lastMigration":null}},"type":"lore"},{"_id":"agkeaco5ANlky2cB","img":"systems/pf2e/icons/default-icons/lore.svg","name":"Athletics","sort":2200000,"system":{"description":{"value":""},"mod":{"value":26},"proficient":{"value":0},"rules":[],"slug":null,"source":{"value":""},"traits":{"rarity":"common","value":[]},"schema":{"version":0.827,"lastMigration":null}},"type":"lore"},{"_id":"fiaMQK3pftNwAxCa","img":"systems/pf2e/icons/default-icons/lore.svg","name":"Occultism","sort":2300000,"system":{"description":{"value":""},"mod":{"value":23},"proficient":{"val