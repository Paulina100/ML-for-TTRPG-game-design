rycasters":{"value":""},"secondarycheck":{"value":""},"slug":"abundant-step","source":{"value":""},"spellType":{"value":"utility"},"sustained":{"value":false},"target":{"value":""},"time":{"value":"1"},"traditions":{"value":[]},"traits":{"rarity":"uncommon","value":["monk","teleportation"]},"schema":{"version":0.827,"lastMigration":null}},"type":"spell"},{"_id":"88CtX72i63mhJTJG","flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.oo7YcRC2gcez81PV"}},"img":"systems/pf2e/icons/spells/ki-blast.webp","name":"Ki Blast","sort":300000,"system":{"ability":{"value":""},"area":null,"category":{"value":"focus"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"cost":{"value":""},"damage":{"value":{"0":{"type":{"categories":[],"value":"force"},"value":"6d6"}}},"description":{"value":"<p>You unleash your ki as a powerful blast of force that deals 2d6 force damage.</p>\n<p>If you use 2 actions to cast ki blast, increase the size of the cone to 30 feet and the damage to [[/r 3d6[force]]].</p>\n<p>If you use 3 actions to cast ki blast, increase the size of the cone to 60 feet and the damage to [[/r 4d6[force]]].</p>\n<p>Each creature in the area must attempt a Fortitude saving throw.</p>\n<hr />\n<p><strong>Critical Success</strong> The creature is unaffected.</p>\n<p><strong>Success</strong> The creature takes half damage.</p>\n<p><strong>Failure</strong> The creature takes full damage and is pushed 5 feet.</p>\n<p><strong>Critical Failure</strong> The creature takes double damage and is pushed 10 feet.</p>\n<hr />\n<p><strong>Heightened (+1)</strong> The damage increases by 1d6, or by [[/r 2d6]] if you use 2 or 3 actions.</p>"},"duration":{"value":""},"hasCounteractCheck":{"value":false},"heightening":{"damage":{"0":"1d6"},"interval":1,"type":"interval"},"level":{"value":7},"location":{"value":"DKsQM7WbtcPBTDCg"},"materials":{"value":""},"prepared":{"value":""},"primarycheck":{"value":""},"range":{"value":""},"rules":[],"save":{"basic":"","value":"f